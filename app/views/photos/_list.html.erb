<% if @photos %>

    <div class="row">
        <% @photos.each do |photos| %>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading preview">
                        <%= image_tag photos.image.url() %>
                    </div>
                </div>
            </div>
        <% end %>
    </div>

<% end %>